task: "thesaurus"
doc: "Given a word or phrase, find alternates"
prompt-version: 1
prompt: |+
  <delim>
  3 other ways of saying "get stuck":
  <delim>
  - paint into a corner
  - become frozen in place
  - become fastened
  <delim>
  10 other ways of saying <q:phrase>:
  <delim>
  -
engine: "OpenAI Codex"
temperature: 0.3
max-generated-tokens: "(* 2 prompt-length)"
top-p: 1.0
stop-sequences:
- "<delim>"
cache: on
vars:
- "phrase"
postprocessor: sed "s/^-\\s*//"
split-patterns:
- "\n"
examples:
- "The misinterpretation"
filter: on
completion: off
insertion: off