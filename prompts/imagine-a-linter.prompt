task: "Imagine an awk linter"
doc: "Given a line of awk code, check it for errors"
prompt-version: 1
prompt: |+
    awk -f program.awk
    awk: /path/to/file:1: if NR >= window_size && (effective_nr % step == 0) {
    awk: /path/to/file:1: ^ syntax error
    awk: /path/to/file:10: <line of code>
    awk: /path/to/file:10: 
engine: "OpenAI Codex"
temperature: 0.2
max-tokens: 60
top-p: 1.0
cache: on
vars:
- "line of code"
var-defaults:
- "(detect-language)"
- "(pen-preceding-text)"
examples:
- "boysenberries"
- "strawberries"
preprocessors:
- "cat"
- "cat"
filter: no
completion: on
insertion: on