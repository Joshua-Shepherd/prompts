task: "transpile"
doc: "Given some code and a target language, transpile into that language"
prompt-version: 3
engine: OpenAI Codex
prompt: |+
    # The program in <from language>

    cat program_in_<from language> <<EOD
    <code>
    EOD

    # The exact same program in <to language>

    cat program_in<to language> <<EOD
n-generate: 5
temperature: 0
max-tokens: 1000
top-p: 1.0
best-of: 1
cache: on
stop-sequences:
- "EOD"
vars:
- code
- "from language"
- "to language"
preprocessors:
- cat
- slugify
- slugify
var-defaults:
- "(pen-selected-text)"
- "(pen-detect-language-ask)"
postprocessors:
- sed '$d'
examples:
- "print(\"Hello world with empathy\")"
- "Python"
- "Ruby"
n-test-runs: 5
n-collate: 1
n-completions: 10
new-document: yes
external-related:
- "https://www.youtube.com/watch?v=Iq3rDFZOorw"
- "https://semiosis.github.io/posts/the-codex-quine/"