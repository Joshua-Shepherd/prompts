title: code generator from description
prompt-version: 1
lm-command: "openai-complete.sh"
model: davinci
# lm-command: "aix-complete.sh"
# model: GPT-J-6B
temperature: 0.3
max-tokens: 200
stop-sequences:
- "###"
top-p: 1
# this flag instructs pen-aix to add a newline before the response
# The AIx API appears to remove all beginning whitespace, which may be a bug
prompt: |+
  Language: `(pen-detect-language)`
  Description: <code description>`(comment-line 2)`
  Code:

  <:pp>
vars:
- code description
postprocessor: sed 1,2d
end-yas: on
# The start will not be trimmed
insertion: on