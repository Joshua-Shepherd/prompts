in-development: yes
title: "break up into sentences"
prompt-version: 1
prompt: |+
    Separate the following paragraph into sentences.
    ###
    <1>
    ###
    <:pp>
prompt-filter: "sed -z 's/\\s\\+$//'"
n-generate: 5
preprocessors:
- "pf-correct-grammar"
lm-command: "openai-complete.sh"
engine: "davinci"
temperature: 0.8
max-tokens: 60
top-p: 1.0
best-of: 1
no-trim-start: off
no-trim-end: off
stop-sequences:
- "\n"
- "\n\n"
- "###"
cache: on
- "former"
- "latter"
var-defaults:
- "(detect-language)"
- "(pen-preceding-text)"
examples:
- "boysenberries"
- "strawberries"
preprocessors:
- "sed 's/^/- /"
- "cat"
aliases:
- "asktutor"
postprocessor: "sed 's/- //' | uniqnosort"
n-test-runs: 5
prettifier: ttp
n-collate: 1
n-completions: 10
pen-collation-postprocessor: "uniqnosort"
filter: no
completion: on
insertion: on

todo:
- Finish this prompt.
design-patterns:
- multiplex
- "https://generative.ink/posts/methods-of-prompt-programming/"
future-titles:
- Get code snippet
- Get snippet
aims:
- More abstractive rewording
doc: "Given ... title"
notes:
- "rlprompt is used here outside of pen.el"
issue:
- "Struggles with the latter columns."
past-versions:
- deprecated/pick-up-line.prompt
external-related:
- "https://paraphrasing-tool.com/"