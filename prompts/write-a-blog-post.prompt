in-development: no
title: "Write a blog post"
prompt-version: 1
prompt: |+
    Write a <2> words blog post about the <1>.

prompt-filter: "sed -z 's/\\s\\+$//'"
lm-command: "openai-complete.sh"
engine: "davinci-instruct-beta"
temperature: 0.8
max-tokens: <2>
top-p: 1.0
best-of: 1
no-trim-start: off
no-trim-end: off
stop-sequences:
- "\n\n"
cache: on
vars:
- "generation task"
examples:
- "Write a 500 words blog post about the iPhone 12."
n-test-runs: 5
prettifier: ttp
n-collate: 1
n-completions: 10
filter: no
completion: off
doc: "Write a blog post"
external-related:
- "https://paraphrasing-tool.com/"