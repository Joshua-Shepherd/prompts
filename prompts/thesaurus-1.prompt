task: "synonym"
aliases:
- thesaurus
doc: "Given a word or phrase, find alternates"
prompt-version: 2
prompt: |+
  Thesaurus:
  <delim>
  3 ways to say "get stuck":
  - paint into a corner
  - become frozen in place
  - become fastened
  <delim>
  5 ways to say <q:phrase>:
  - 
engine: "OpenAI Codex"
temperature: 0.3
max-generated-tokens: "(* 2 prompt-length)"
top-p: 1.0
n-completions: 5
stop-sequences:
- "\n\n"
cache: on
vars:
- "phrase"
postprocessor: sed "s/^-*\\s*//" | sed -e "s/^\"\\(.*\\)\"$/\\1/" -e "s/^\"$//"
split-patterns:
- "\n"
examples:
- "The misinterpretation"
filter: on
completion: off
insertion: off