include: Generic Interpreter
task: Imagine a <language> interpreter
language: scheme
prompt: |+
    Chez Scheme Version 9.5.3
    Copyright 1984-2019 Cisco Systems, Inc.
    
    > <expression>
user-prompt: "^> "
vars:
- "expression"
examples:
- "(+ 5 5)"
n-completions: 10
validator: head -n 1 | grep -qP "^[^>]"