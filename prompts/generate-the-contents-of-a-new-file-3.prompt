task: "Generate the contents of a new file"
doc: "Prompt for the probable contents of a file with this path and sibling files"
prompt-version: 1
prompt: |+
    $ ls | head -n 10
    <ls output>

    $ cat <q:path> <<EOD
    <preceding text>
engine: "OpenAI Codex"
temperature: 0.3
max-generated-tokens: 150
top-p: 1.0
# This means that even a long-completion can't change the stop-sequence
force-stop-sequence: EOD
stop-sequences:
- EOD
cache: on
vars:
- "preceding text"
- "path"
- "ls output"
var-defaults:
- "(pen-preceding-text)"
- "(f-basename (get-path))"
- "(pen-snc \"ls | head -n 10\")"
examples:
- ""
- ".gitignore"
- ".\nLICENSE.md"
filter: off
info: off
completion: on
insertion: on