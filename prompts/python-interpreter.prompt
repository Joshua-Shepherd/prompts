task: "Python Interpreter"
doc: "Imagine a Python interpreter"
prompt-version: 1
prompt: |+
    Python 3.8.5 (default, Jan 27 2021, 15:41:15)
    Type 'copyright', 'credits' or 'license' for more information
    IPython 7.21.0 -- An enhanced Interactive Python. Type '?' for help.
    
    In [1]: <expression>
lm-command: "openai-complete.sh"
engine: "OpenAI Davinci"
temperature: 0.4
max-tokens: 60
top-p: 1.0
cache: on
stop-sequences:
- "]: "
stop-patterns:
- "^In \\[[0-9]*\\]: "
vars:
- "expression"
- "latter"
examples:
- "5 + 5"
filter: no
completion: on
insertion: on
external-related:
- "https://paraphrasing-tool.com/"
preprocessors:
- pen-s onelineify
postprocessor: pen-s unonelineify | pen-pretty-paragraph
