task: "imagine a project template"
doc: "Given the type of project I want to make, generate the file structure"
prompt-version: 1
prompt: |+
    ~$ git checkout "https://github.com/templates/<type>-project-template"
    Cloning into '<type>-project-template'...
    remote: Enumerating objects: 306, done.
    remote: Counting objects: 100% (306/306), done.
    remote: Compressing objects: 100% (166/166), done.
    remote: Total 306 (delta 177), reused 244 (delta 115), pack-reused 0
    Receiving objects: 100% (306/306), 5.27 MiB | 1.40 MiB/s, done.
    Resolving deltas: 100% (177/177), done.
    ~$ cd "<type>-project-template"
    ~/<type>-project-template$ ls -R .
    .:
engine: "OpenAI Codex"
temperature: 0.3
max-generated-tokens: "(* 2 prompt-length)"
top-p: 1.0
stop-sequences:
- "~/"
cache: on
vars:
- "type"
examples:
- "haskell stack"
info: on
completion: off
insertion: off