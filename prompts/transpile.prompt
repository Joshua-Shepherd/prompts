title: "transpile"
doc: "Given some code and a target language, transpile into that language"
prompt-version: 1
engine: openai-codex
prompt: |+
    ## The program in <from language>

    cat program_in_<language> <<EOD
    <code>
    EOD

    ## The exact same program in <to-language>

n-generate: 5
temperature: 0
max-tokens: 1000
top-p: 1.0
best-of: 1
cache: on
vars:
- code
- "from language"
- "to language"
var-defaults:
- "(pen-selected-text)"
- "(pen-detect-language-ask)"
examples:
- "print(\"Hello world with empathy\")"
- "Python"
- "Ruby"
n-test-runs: 5
n-collate: 1
n-completions: 10
new-document: yes
external-related:
- "https://www.youtube.com/watch?v=Iq3rDFZOorw"