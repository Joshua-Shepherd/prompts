title: "transpile"
prompt-version: 1
engine: openai-codex
prompt: |+
    ## The program in <from language>

    cat program_in_<language> <<EOD
    <code>
    EOD

    ## The exact same program in <to-language>

    <1> are like <2> in that
n-generate: 5
preprocessors:
- "pf-correct-grammar"
lm-command: "openai-complete.sh"
model: "davinci"
temperature: 0
max-tokens: 1000
top-p: 1.0
best-of: 1
cache: on
vars:
- "from language"
- "to language"
var-defaults:
- "(detect-language)"
- "(pen-preceding-text)"
examples:
- "boysenberries"
- "strawberries"
preprocessors:
- "sed 's/^/- /"
- "cat"
aliases:
- "asktutor"
postprocessor: "sed 's/- //' | uniqnosort"
n-test-runs: 5
prettifier: pen-pretty-paragraph
n-collate: 1
n-completions: 10
pen-collation-postprocessor: "uniqnosort"
filter: no
completion: on
insertion: on

todo:
- Finish this prompt.
design-patterns:
- multiplex
- "https://generative.ink/posts/methods-of-prompt-programming/"
future-titles:
- Get code snippet
- Get snippet
aims:
- More abstractive rewording
doc: "Given ... title"
notes:
- "rlprompt is used here outside of pen.el"
issue:
- "Struggles with the latter columns."
past-versions:
- deprecated/pick-up-line.prompt
external-related:
- "https://paraphrasing-tool.com/"