task: "Instruct an AI to do write something"
doc: "Provide a natural language task such as 'Explain the moon landing to a 6 year old in a few sentences.'"
prompt-version: 1
prompt: |+
  <instruction>
engine: "OpenAI Davinci Instruct"
force-engine: "OpenAI Davinci Instruct"
temperature: 0.3
max-generated-tokens: "(* 2 prompt-length)"
top-p: 1.0
stop-sequences:
- "<delim>"
cache: on
vars:
- instruction
examples:
- "Explain the moon landing to a 6 year old in a few sentences."
filter: on
completion: off
insertion: off
prettifier: pen-pretty-paragraph