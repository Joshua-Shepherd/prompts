title: "Explain code given a language"
doc: "Select some code and ask for an explanation"
is-info: on
prompt-version: 2
subprompts:
- metadata: |+
    File type: <language>
    File contents:
prompt: |+
    <metadata>
    """
    <code>
    """
    Summary of the above code:
    """
xlr-n-generate: 5
engine: "OpenAI Codex"
temperature: 0.3
max-generated-tokens: "(* 2 prompt-length)"
top-p: 1.0
best-of: 1
no-trim-start: off
no-trim-end: off
preprocessors:
- sed 's="""=\\"\\"\\"='
stop-sequences:
- "\"\"\""
yas: on
vars:
- code
- language
examples:
- |
    var myDraw = Draw(200, 200, true);
    var x1 = randomInteger(200);
    var y1 = randomInteger(200);
    var x2 = randomInteger(200);
    var y2 = randomInteger(200);
    myDraw.line(x1, y1, x2, y2);
n-test-runs: 5
n-collate: 1
n-completions: 5
filter: no
completion: off
insertion: off