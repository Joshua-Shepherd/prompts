engine: myrc
preferred-openai-engine: davinci
temperature: 0.5
max-tokens: 60
top-p: 1
frequency-penalty: 0.5
presence-penalty: 0
best-of: 1
stop-sequences:
- "###"
cache: true
vars:
- Artist/Album
examples:
- Beatles
chomp-start: true
chomp-end: false
prefer-external: true
conversation-mode: false
filter: false
postprocessor: sed '/^- /s/- //p' | sed '/^-/d' | sed -n '/^[a-zA-Z0-9 _-]\+$/p' |
  uniqnosort
pen-collation-postprocessor: uniqnosort
