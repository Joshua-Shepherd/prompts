task: "Get URLs for a passage"
doc: "Given some text return relevant URLs"
prompt-version: 1
prompt: |+
  """
  <text>
  """
  Relevant URLs:
engine: "OpenAI Codex"
temperature: 0.3
max-generated-tokens: "(* 2 prompt-length)"
top-p: 1.0
stop-sequences:
- "###"
cache: on
vars:
- text
var-defaults:
- "(sor (pen-selected-text) (pen-preceding-text))"
examples:
- |-
    import Data.Foldable (for_)
    import Data.Traversable (for)
    
    import Control.Monad (when)
info: on
completion: off
insertion: off