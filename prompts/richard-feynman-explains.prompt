title: "Richard Feynman explains"
doc: "Richard Feynman explains anything"
prompt-version: 2
prompt: |+
  This is an except from Richard Feynman's lecture.
  Questions:
  "Could you please explain how and why <question>".
  Richard Feynman:
  "
lm-command: "openai-complete.sh"
model: "davinci"
temperature: 0.8
max-tokens: 200
top-p: 1.0
cache: on
preprocessors:
- chomp
stop-sequences:
- "\""
vars:
- "question"
examples:
- "Why is the sky blue?"
filter: yes
postprocessor: pen-s join ' ' | sed -z -e "s/\\.*$//" -e "s/$/./" | pen-s capfirst