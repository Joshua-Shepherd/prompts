task: "NLP unparser: Generate text using POS tags"
doc: "Generate text given a list of POS tags"
prompt-version: 1
prompt: |+
  Parse tree and matching passage:
  <delim>
  1) Parts of speech tagging:
  <delim>
  NP V Det N
  <delim>
  John(NP) hit(V) the(Det) ball(N)
  <delim>
  NP V Det N
  <delim>  
  2) Parts of speech tagging:
  <delim>
  NP V Det N IN Det N
  <delim>
  I(NP) drive(V) a(Det) car(N) to(IN) my(Det) college(N)
  <delim>
  NP V Det N IN Det N
  <delim>
  3) Parts of speech tagging:
  <delim>
  <POS tags>
  <delim>
engine: "OpenAI Codex"
temperature: 0.3
max-generated-tokens: "(* 2 prompt-length)"
top-p: 1.0
stop-sequences:
- "<delim>"
cache: on
vars:
- "POS tags"
examples:
- "V Det N NP"
filter: on
completion: off
insertion: off