task: "NLP unparser: Generate text using POS tags"
doc: "Generate text given a list of POS tags"
prompt-version: 1
prompt: |+
  Parse tree and matching passage:
  <delim>
  1) Parts of speech tags:
  <delim>
  NP V Det N
  <delim>
  1) Passage:
  <delim>
  John hit the ball
  <delim>  
  2) Parts of speech tags:
  <delim>
  <POS tags>
  <delim>
  2) Passage:
  <delim>
engine: "OpenAI Codex"
temperature: 0.3
max-generated-tokens: "(* 2 prompt-length)"
top-p: 1.0
stop-sequences:
- "<delim>"
cache: on
vars:
- "POS tags"
examples:
- "V Det N NP"
filter: on
completion: off
insertion: off